[group]
id=${index}
timestamp=${time_stamp}

[consensus]
    ; consensus algorithm type, now support PBFT(consensus_type=pbft) and Raft(consensus_type=raft)
    consensus_type=pbft
    ; the max number of transactions of a block
    max_trans_num=1000
    node.0=

[state]
    type=storage

; tx gas limit
[tx]
    gas_limit=300000000
